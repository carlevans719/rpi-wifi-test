  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "trace": "all",
      "name": "Debug Meteor App",
      "port": 5959,
      "timeout": 20000,
      "restart": true,
      "sourceMaps": true,
